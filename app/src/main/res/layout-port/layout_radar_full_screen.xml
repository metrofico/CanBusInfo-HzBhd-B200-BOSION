            android:layout_height="wrap_content"
            android:src="@drawable/left_radar_rear_1"
            android:layout_below="@+id/radar_big_car"
            android:layout_centerHorizontal="true"/>
        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp38"
            android:layout_alignParentBottom="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/warning_icon"/>
            <TextView
                android:textSize="@dimen/dp35"
                android:textColor="@color/white"
                android:id="@+id/tv_bottom_warning_tis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp38"/>
        </LinearLayout>
