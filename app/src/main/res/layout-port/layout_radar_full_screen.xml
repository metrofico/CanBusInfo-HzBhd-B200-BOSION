                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/r_l_txt"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp60"
                    android:text=""/>
            </LinearLayout>
        </RelativeLayout>
        <TextView
