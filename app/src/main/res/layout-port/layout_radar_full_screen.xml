            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/left_radar_front_mid_1"
            android:layout_below="@+id/radar_big_car"
            android:layout_centerHorizontal="true"/>
        <ImageView
            android:id="@+id/left_radar_rear_mid"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/left_radar_rear_mid_1"
