            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_23"
            android:layout_marginTop="@dimen/dp346"
            android:src="@drawable/back_radar_left_0"
            android:layout_toRightOf="@+id/radar_big_car"/>
        <ImageView
            android:id="@+id/back_radar_left_probe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_40"
            android:layout_marginTop="@dimen/dp390"
            android:src="@drawable/back_radar_left_0"
            android:layout_toRightOf="@+id/radar_big_car"
            android:rotation="48"/>
        <RelativeLayout
            android:id="@+id/top_radar"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/right_radar_front"
            android:layout_alignStart="@+id/right_radar_front">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/t_l_txt"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp103"
                    android:layout_height="@dimen/dp60"
                    android:text=""
                    android:layout_marginEnd="@dimen/dp80"/>
                <TextView
                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/t_l_m_txt"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp103"
                    android:layout_height="@dimen/dp60"
                    android:text=""
                    android:layout_marginEnd="@dimen/dp80"/>
