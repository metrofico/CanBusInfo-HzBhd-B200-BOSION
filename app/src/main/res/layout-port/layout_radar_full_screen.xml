            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/right_radar_rear_1"
            android:layout_above="@+id/radar_big_car"
            android:layout_centerHorizontal="true"/>
        <RelativeLayout
            android:id="@+id/bottom_radar"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/left_radar_front"
            android:layout_alignStart="@+id/left_radar_front">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/b_l_txt"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp103"
                    android:layout_height="@dimen/dp60"
                    android:text=""
                    android:layout_marginEnd="@dimen/dp80"/>
                <TextView
                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/b_l_m_txt"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp103"
                    android:layout_height="@dimen/dp60"
                    android:text=""
                    android:layout_marginEnd="@dimen/dp80"/>
                <TextView
                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/b_r_m_txt"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp103"
                    android:layout_height="@dimen/dp60"
                    android:text=""
                    android:layout_marginEnd="@dimen/dp80"/>
                <TextView
                    android:textSize="@dimen/dp30"
                    android:textColor="@android:color/white"
                    android:id="@+id/b_r_txt"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp103"
                    android:layout_height="@dimen/dp60"
                    android:text=""/>
            </LinearLayout>
        </RelativeLayout>
        <ImageView
            android:id="@+id/left_radar_front"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/left_radar_front_1"
