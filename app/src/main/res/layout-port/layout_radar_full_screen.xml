            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp113"
            android:layout_marginRight="@dimen/dp_89"
            android:layout_toLeftOf="@+id/radar_big_car"
            android:rotation="45"/>
        <ImageView
            android:id="@+id/front_radar_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp182"
            android:layout_marginRight="@dimen/dp_28"
            android:layout_toLeftOf="@+id/radar_big_car"/>
        <ImageView
            android:id="@+id/front_radar_right_mid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp244"
            android:layout_marginRight="@dimen/dp_17"
            android:layout_toLeftOf="@+id/radar_big_car"/>
        <ImageView
            android:id="@+id/front_radar_left_mid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp310"
            android:layout_marginRight="@dimen/dp_17"
            android:src="@drawable/front_radar_left_mid_0"
            android:layout_toLeftOf="@+id/radar_big_car"/>
        <ImageView
            android:id="@+id/front_radar_left"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp350"
            android:layout_marginRight="@dimen/dp_28"
            android:src="@drawable/front_radar_left_0"
