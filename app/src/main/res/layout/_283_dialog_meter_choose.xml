            android:visibility="gone"
            android:text="@string/ACC"
            android:drawableTop="@drawable/_a6_meter_choose_10"
            app:layout_constraintStart_toEndOf="@+id/meterChooseTv9"
            app:layout_constraintTop_toTopOf="@+id/meterChooseTv9"
