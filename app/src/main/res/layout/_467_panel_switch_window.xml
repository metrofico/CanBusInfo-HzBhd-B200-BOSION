<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/round_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:background="@drawable/_467_alert_bg_img"
        android:layout_width="734px"
        android:layout_height="338px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textSize="30px"
            android:textColor="#ffffff"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="60px"
            android:layout_margin="20px"
            android:text="@string/_439_panel_app_show_hide_switch"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"/>
        <com.hzbhd.canbus.car_cus._467.air.view.CheckItemView2
            android:id="@+id/on_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80px"
            android:layout_marginRight="80px"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"/>
        <com.hzbhd.canbus.car_cus._467.air.view.CheckItemView2
            android:id="@+id/off_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80px"
            android:layout_marginRight="80px"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"/>
        <LinearLayout
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px">
            <Button
                android:textSize="28px"
                android:textColor="@color/white"
                android:id="@+id/reset"
                android:background="@drawable/_467_button_up_down"
                android:layout_width="140px"
                android:layout_height="62px"
                android:layout_marginRight="10px"
                android:text="@string/_439_panel_app2"
                android:singleLine="true"/>
            <Button
                android:textSize="28px"
                android:textColor="@color/white"
                android:id="@+id/cancel"
                android:background="@drawable/_467_button_up_down"
                android:layout_width="140px"
                android:layout_height="62px"
                android:layout_marginLeft="10px"
                android:text="@string/_439_fun_04"/>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
