<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView style="@style/_283_scrollView">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.hzbhd.canbus.car_cus._283.view.SettingSelectView
                android:id="@+id/ssv_assist"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_1"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingSelectView
                android:id="@+id/ssv_bend"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_2"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingSelectView
                android:id="@+id/ssv_laneChange"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_change_lanes"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingSelectView
                android:id="@+id/ssv_automatic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_3"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingSelectView
                android:id="@+id/ssv_day"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_4"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingDialogView
                android:id="@+id/sdv_openTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_5"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingDialogView
                android:id="@+id/sdv_travel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_travel_mode"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_big_light"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_lighting_title_1"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_light_system"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_lighting_title_2"/>
            <TextView
                android:text="@string/_283_car_setting_in_car_light"
                style="@style/_283_car_setting_little_title"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_instrument"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_switch"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_door"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_door_light"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_footwell"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_6"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/sdv_coming"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_7"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/sdv_leaving"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_light_8"/>
            <TextView
                android:text="@string/_283_car_setting_ambient_lighting"
                style="@style/_283_car_setting_little_title"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingDialogView
                android:id="@+id/sdv_ambient_three"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_ambient_lighting_three"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingDialogView
                android:id="@+id/sdv_ambient_ten"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_ambient_lighting_ten"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingDialogView
                android:id="@+id/sdv_ambient_twenty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_ambient_lighting_twenty"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_ambient_car_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_ambient_lighting_title_1"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_ambient_car_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_ambient_lighting_title_2"/>
            <com.hzbhd.canbus.car_cus._283.view.SettingProgressView
                android:id="@+id/spv_ambient_car_all"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:setting_title="@string/_283_car_setting_ambient_lighting_title_3"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</LinearLayout>
