<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:bhd="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/out_view_lin"
        android:background="@color/white"
        android:layout_width="600px"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="40px"
            android:gravity="center_vertical"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:layout_marginLeft="20px"
            android:text="@string/_439_air_title_00"/>
        <View
            android:background="#424242"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <com.hzbhd.canbus.car_cus._439.air.view.CheckItemView
            android:id="@+id/c1"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:layout_marginLeft="60px"/>
        <com.hzbhd.canbus.car_cus._439.air.view.CheckItemView
            android:id="@+id/c2"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:layout_marginLeft="60px"/>
        <com.hzbhd.canbus.car_cus._439.air.view.CheckItemView
            android:id="@+id/c3"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="100px"
            android:layout_marginLeft="60px"/>
        <View
            android:background="#424242"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <TextView
            android:textSize="40px"
            android:gravity="center_vertical"
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginLeft="20px"
            android:text="@string/_439_air_title_01"/>
        <View
            android:background="#424242"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="60px"
            android:layout_marginTop="40px"
            android:layout_marginRight="10px"
            android:layout_marginBottom="20px">
            <TextView
                android:textSize="40px"
                android:gravity="center_vertical"
                android:id="@+id/setTimeTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/_439_air_title_02"/>
            <LinearLayout
                android:textSize="28px"
                android:textColor="@color/black"
                android:textColorHint="@color/grey"
                android:gravity="center"
                android:orientation="horizontal"
                android:background="@drawable/__439__kongt_shuchuk"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="28px"
                    android:textColor="@color/black"
                    android:textColorHint="@color/grey"
                    android:id="@+id/settings_time_hour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="number"/>
                <TextView
                    android:textSize="28px"
                    android:textColor="@color/black"
                    android:textColorHint="@color/grey"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:inputType="number"
                    android:layout_marginStart="10dp"/>
                <TextView
                    android:textSize="28px"
                    android:textColor="@color/black"
                    android:textColorHint="@color/grey"
                    android:id="@+id/settings_time_min"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:layout_marginStart="10dp"/>
            </LinearLayout>
            <TextView
                android:textSize="28px"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/set_time_open"
                android:background="@drawable/__439__kongt_anjian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10px"
                android:text="@string/_375_car_setting613"/>
        </LinearLayout>
        <TextView
            android:textSize="22px"
            android:textColor="@color/red"
            android:id="@+id/alert"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="60px"
            android:layout_marginRight="10px"
            android:text="注意：ggggggggggggggg"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="60px"
            android:layout_marginLeft="60px"
            android:layout_marginTop="20px"
            android:layout_marginRight="10px"
            android:layout_marginBottom="40px">
            <TextView
                android:textSize="40px"
                android:gravity="center_vertical"
                android:id="@+id/currentTimeTitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/_439_air_title_04"/>
            <TextView
                android:textSize="40px"
                android:gravity="center_vertical"
                android:id="@+id/resultTime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="------"
                android:layout_weight="1"/>
            <TextView
                android:textSize="28px"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/cancel"
                android:background="@drawable/__439__kongt_anjian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10px"
                android:text="@string/_439_fun_04"/>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout
        android:orientation="vertical"
        android:id="@+id/setting_time"
        android:background="@color/white"
        android:visibility="gone"
        android:layout_width="800px"
        android:layout_height="420dp">
        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="250dp">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <TextView
                    android:textSize="28px"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:id="@+id/time_day"
                    android:background="@drawable/__439__kongt_shuchuk_3"
                    android:layout_width="270dp"
                    android:layout_height="60dp"
                    android:text="@string/_439_time_tobay"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:id="@+id/time_day_line"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <com.hzbhd.ui.view.colorview.ColorView
                        android:textSize="28px"
                        android:textColor="@color/black"
                        android:gravity="center"
                        android:id="@+id/time_today"
                        android:layout_width="270dp"
                        android:layout_height="60dp"
                        android:text="@string/_439_time_tobay"
                        bhd:n="@drawable/transparent"
                        bhd:p="@drawable/__439__kongt_shuchuk"
                        bhd:s="@drawable/__439__kongt_shuchuk"/>
                    <ImageView
                        android:background="@drawable/dz_radar_r_line"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"/>
                    <com.hzbhd.ui.view.colorview.ColorView
                        android:textSize="28px"
                        android:textColor="@color/black"
                        android:gravity="center"
                        android:id="@+id/time_tomorrow"
                        android:layout_width="270dp"
                        android:layout_height="60dp"
                        android:text="@string/_439_time_tomorrow"
                        bhd:n="@drawable/transparent"
                        bhd:p="@drawable/__439__kongt_shuchuk"
                        bhd:s="@drawable/__439__kongt_shuchuk"/>
                    <ImageView
                        android:background="@drawable/dz_radar_r_line"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"/>
                </LinearLayout>
            </LinearLayout>
            <EditText
                android:textSize="28px"
                android:textColor="@color/black"
                android:textColorHint="@color/grey"
                android:gravity="center"
                android:id="@+id/time_hour"
                android:background="@drawable/__439__kongt_shuchuk"
                android:layout_width="@dimen/dp135"
                android:layout_height="wrap_content"
                android:hint="0-24H"
                android:inputType="number"
                android:layout_marginStart="40dp"/>
            <TextView
                android:textSize="28px"
                android:textColor="@color/black"
                android:textColorHint="@color/grey"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=":"
                android:inputType="number"
                android:layout_marginStart="10dp"/>
            <EditText
                android:textSize="28px"
                android:textColor="@color/black"
                android:textColorHint="@color/grey"
                android:gravity="center"
                android:id="@+id/time_minute"
                android:background="@drawable/__439__kongt_shuchuk"
                android:layout_width="@dimen/dp135"
                android:layout_height="wrap_content"
                android:hint="0-60Min"
                android:inputType="number"
                android:layout_marginStart="10dp"/>
            <TextView
                android:textSize="28px"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/setTime"
                android:background="@drawable/__439__kongt_anjian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10px"
                android:text="@string/_439_air_title_03"
                android:layout_marginStart="20dp"/>
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
