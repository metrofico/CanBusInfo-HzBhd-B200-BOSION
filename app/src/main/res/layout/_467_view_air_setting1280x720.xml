<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/out_view_lin"
        android:layout_width="500px"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="32px"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginLeft="20px"
            android:text="@string/_439_air_title_00"/>
        <View
            android:background="#424242"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <com.hzbhd.canbus.car_cus._467.air.view.CheckItemView
            android:id="@+id/c1"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginLeft="30px"/>
        <com.hzbhd.canbus.car_cus._467.air.view.CheckItemView
            android:id="@+id/c2"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginLeft="30px"/>
        <com.hzbhd.canbus.car_cus._467.air.view.CheckItemView
            android:id="@+id/c3"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginLeft="30px"/>
        <View
            android:background="#424242"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <TextView
            android:textSize="32px"
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="80px"
            android:layout_marginLeft="20px"
            android:text="@string/_439_air_title_01"/>
        <View
            android:background="#424242"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30px"
            android:layout_marginTop="20px"
            android:layout_marginRight="10px"
            android:layout_marginBottom="20px">
            <TextView
                android:textSize="32px"
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/_439_air_title_02"/>
            <EditText
                android:textSize="28px"
                android:textColor="@color/black"
                android:textColorHint="@color/grey"
                android:gravity="center"
                android:id="@+id/time"
                android:background="@drawable/__439__kongt_shuchuk"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:hint="0-24H"
                android:layout_weight="1"
                android:inputType="number"/>
            <TextView
                android:textSize="28px"
                android:textColor="@color/white"
                android:gravity="center"
                android:id="@+id/setTime"
                android:background="@drawable/__439__kongt_anjian"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10px"
                android:text="@string/_439_air_title_03"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30px"
            android:layout_marginTop="10px"
            android:layout_marginRight="10px"
            android:layout_marginBottom="20px">
            <TextView
                android:textSize="32px"
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="46px"
                android:text="@string/_439_air_title_04"/>
            <TextView
                android:textSize="32px"
                android:gravity="center_vertical"
                android:id="@+id/resultTime"
                android:layout_width="match_parent"
                android:layout_height="46px"
                android:text="@string/_439_air_title_05"/>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
