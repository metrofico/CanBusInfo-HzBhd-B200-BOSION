<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/air_conditioning_bg"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:background="@drawable/dz_setting_bar_gray"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <ImageView
        android:id="@+id/front_left_seat"
        android:layout_width="223dp"
        android:layout_height="155dp"
        android:layout_marginBottom="74dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="303dp"/>
    <ImageView
        android:id="@+id/front_right_seat"
        android:layout_width="123dp"
        android:layout_height="103dp"
        android:layout_marginTop="308dp"
        android:layout_marginStart="548dp"/>
    <ImageView
        android:id="@+id/iv_left_foot"
        android:visibility="gone"
        android:layout_width="94dp"
        android:layout_height="87dp"
        android:layout_marginTop="410dp"
        android:src="@drawable/dz_air1_left_foot"
        android:layout_marginStart="180dp"/>
    <ImageView
        android:id="@+id/iv_left_head"
        android:visibility="gone"
        android:layout_width="199dp"
        android:layout_height="19dp"
        android:layout_marginTop="275dp"
        android:src="@drawable/dz_air1_left_head"
        android:layout_marginStart="125dp"/>
    <ImageView
        android:id="@+id/iv_left_windows"
        android:visibility="gone"
        android:layout_width="68dp"
        android:layout_height="80dp"
        android:layout_marginTop="95dp"
        android:src="@drawable/dz_air1_left_window"
        android:layout_marginStart="170dp"/>
    <ImageView
        android:id="@+id/iv_right_foot"
        android:visibility="gone"
        android:layout_width="71dp"
        android:layout_height="89dp"
        android:layout_marginTop="250dp"
        android:src="@drawable/dz_air1_right_foot"
        android:layout_marginStart="480dp"/>
    <ImageView
        android:id="@+id/iv_right_head"
        android:visibility="gone"
        android:layout_width="250dp"
        android:layout_height="35dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/dz_air1_right_head"
        android:layout_marginStart="560dp"/>
    <ImageView
        android:id="@+id/iv_right_windows"
        android:visibility="gone"
        android:layout_width="66dp"
        android:layout_height="67dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/dz_air1_right_window"
        android:layout_marginStart="350dp"/>
    <TextView
        android:textSize="36dp"
        android:textColor="@color/white"
        android:id="@+id/front_right_airTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginStart="520dp"/>
    <TextView
        android:textSize="36dp"
        android:textColor="@color/white"
        android:id="@+id/front_left_airTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="225dp"
        android:layout_marginStart="110dp"/>
    <com.hzbhd.canbus.car_cus._291.view.WindSpeedView
        android:id="@+id/wsv_left_Wind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="360dp"
        android:layout_marginStart="305dp"/>
    <com.hzbhd.canbus.car_cus._291.view.WindSpeedView
        android:id="@+id/wsv_right_Wind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:layout_marginStart="450dp"/>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_alignParentRight="true">
        <RelativeLayout
            android:gravity="center"
            android:background="@drawable/dz_air_r_bg"
            android:layout_width="146dp"
            android:layout_height="67dp">
            <com.hzbhd.canbus.car_cus._291.view.BtnView
                android:id="@+id/btn_ac"
                android:layout_width="61dp"
                android:layout_height="44dp"
                app:dz_no_select_ic="@drawable/dz_air_wind_ac_n"
                app:dz_select_ic="@drawable/dz_air_wind_ac_p"/>
        </RelativeLayout>
        <RelativeLayout
            android:gravity="center"
            android:background="@drawable/dz_air_r_bg"
            android:layout_width="146dp"
            android:layout_height="67dp"
            android:layout_marginTop="-4dp">
            <com.hzbhd.canbus.car_cus._291.view.BtnView
                android:id="@+id/btn_cycle"
                android:layout_width="61dp"
                android:layout_height="44dp"
                app:dz_no_select_ic="@drawable/dz_air_outer_loop_p"
                app:dz_select_ic="@drawable/dz_air_outer_loop_p"/>
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:background="@drawable/dz_seat_bottom_bg"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true">
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_off_on"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/_291_dz_air_bottom_icon_off_n"
            app:dz_select_ic="@drawable/_291_dz_air_bottom_icon_off_p"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_vehicle_information_bottom_line"/>
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_dual"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/dz_air_bottom_icon_dual_n"
            app:dz_select_ic="@drawable/dz_air_bottom_icon_dual_p"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_vehicle_information_bottom_line"/>
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_auto"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/dz_air_bottom_icon_auto_n"
            app:dz_select_ic="@drawable/dz_air_bottom_icon_auto_p"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_vehicle_information_bottom_line"/>
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_max_ac"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/dz_air_bottom_icon_rear_maxac_n"
            app:dz_select_ic="@drawable/dz_air_bottom_icon_rear_maxac_p"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_vehicle_information_bottom_line"/>
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_max"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/dz_air_bottom_icon_rear_max_n"
            app:dz_select_ic="@drawable/dz_air_bottom_icon_rear_max_p"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_vehicle_information_bottom_line"/>
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_rear"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/dz_air_l_icon_rear_n"
            app:dz_select_ic="@drawable/dz_air_l_icon_rear_p"/>
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_vehicle_information_bottom_line"/>
        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1"/>
        <com.hzbhd.canbus.car_cus._291.view.BtnView
            android:id="@+id/btn_auto2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:dz_no_select_ic="@drawable/dz_air_bottom_icon_rear_auto2_n"
            app:dz_select_ic="@drawable/dz_air_bottom_icon_rear_auto2_p"/>
    </LinearLayout>
</RelativeLayout>
