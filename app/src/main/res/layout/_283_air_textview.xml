<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/dp270"
    android:layout_height="@dimen/dp110">
    <ImageView
        android:background="@drawable/dz_air_big_bg"
        android:layout_width="@dimen/dp250"
        android:layout_height="@dimen/dp65"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/dp10"
        android:layout_marginEnd="@dimen/dp10"/>
    <com.hzbhd.canbus.car_cus._283.view.BtnView
        android:id="@+id/subBtnView"
        android:paddingTop="@dimen/dp23"
        android:paddingBottom="@dimen/dp23"
        android:layout_width="@dimen/dp85"
        android:layout_height="@dimen/dp86"
        android:layout_centerVertical="true"
        android:paddingStart="@dimen/dp20"
        android:paddingEnd="@dimen/dp20"
        app:dz_no_select_ic="@drawable/dz_air_less_n"
        app:dz_select_ic="@drawable/dz_air_less_p"/>
    <com.hzbhd.canbus.car_cus._283.view.MyTextView
        android:gravity="center"
        android:layout_gravity="center_vertical"
        android:id="@+id/myTextView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_centerInParent="true"/>
    <com.hzbhd.canbus.car_cus._283.view.BtnView
        android:id="@+id/addBtnView"
        android:paddingTop="@dimen/dp23"
        android:paddingBottom="@dimen/dp23"
        android:layout_width="@dimen/dp85"
        android:layout_height="@dimen/dp86"
        android:layout_centerVertical="true"
        android:paddingStart="@dimen/dp20"
        android:paddingEnd="@dimen/dp20"
        android:layout_alignParentEnd="true"
        app:dz_no_select_ic="@drawable/dz_air_plus_n"
        app:dz_select_ic="@drawable/dz_air_plus_p"/>
</RelativeLayout>
