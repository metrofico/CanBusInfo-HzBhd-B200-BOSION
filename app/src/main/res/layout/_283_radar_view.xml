<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayout"
    android:background="@null"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/showCarRadar"
        android:visibility="gone"
        android:layout_width="102dp"
        android:layout_height="72dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/_283_radar_hide"/>
    <RelativeLayout
        android:id="@+id/LeftRelative"
        android:background="@drawable/dz_radar_r_w_bg"
        android:layout_width="498dp"
        android:layout_height="600dp">
        <ImageView
            android:id="@+id/leftClose"
            android:visibility="gone"
            android:layout_width="102dp"
            android:layout_height="72dp"
            android:layout_margin="10dp"
            android:src="@drawable/_283_radar_close"
            android:layout_alignParentRight="true"/>
        <ImageView
            android:id="@+id/hideCarRadar"
            android:layout_width="102dp"
            android:layout_height="72dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/_283_radar_show"
            android:layout_below="@+id/leftClose"
            android:layout_alignParentRight="true"/>
        <ImageView
            android:id="@+id/iv_radar_stop"
            android:layout_width="105dp"
            android:layout_height="72dp"
            android:src="@drawable/_283_radar_2"
            android:layout_above="@+id/iv_radar_voice"
            android:layout_marginEnd="10dp"
            android:layout_alignParentEnd="true"/>
        <ImageView
            android:id="@+id/iv_radar_voice"
            android:layout_width="105dp"
            android:layout_height="72dp"
            android:layout_margin="10dp"
            android:src="@drawable/_283_radar_1"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/leftRadarView"
        android:layout_width="380dp"
        android:layout_height="600dp">
        <ImageView
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_radar_gray_1"
            android:layout_centerInParent="true"
            android:alpha="0.5"/>
        <ImageView
            android:id="@+id/front_radar_left_mid"
            android:layout_width="79dp"
            android:layout_height="108dp"
            android:layout_marginTop="30dp"
            android:layout_marginStart="110dp"/>
        <ImageView
            android:id="@+id/front_radar_left"
            android:layout_width="83dp"
            android:layout_height="83dp"
            android:layout_marginTop="68dp"
            android:layout_marginStart="78dp"/>
        <ImageView
            android:id="@+id/left_radar_front"
            android:layout_width="83dp"
            android:layout_height="76dp"
            android:layout_marginTop="108dp"
            android:layout_marginStart="58dp"/>
        <ImageView
            android:id="@+id/left_radar_mid_front"
            android:layout_width="69dp"
            android:layout_height="97dp"
            android:layout_marginTop="2dp"
            android:layout_below="@+id/left_radar_front"
            android:layout_marginStart="58dp"/>
        <ImageView
            android:id="@+id/left_radar_mid_rear"
            android:layout_width="69dp"
            android:layout_height="97dp"
            android:layout_marginTop="2dp"
            android:layout_below="@+id/left_radar_mid_front"
            android:layout_marginStart="58dp"/>
        <ImageView
            android:id="@+id/left_radar_rear"
            android:layout_width="85dp"
            android:layout_height="76dp"
            android:layout_marginTop="2dp"
            android:layout_below="@+id/left_radar_mid_rear"
            android:layout_marginStart="58dp"/>
        <ImageView
            android:id="@+id/rear_radar_left"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:layout_marginBottom="100dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="77dp"/>
        <ImageView
            android:id="@+id/rear_radar_left_auxiliary"
            android:layout_width="106dp"
            android:layout_height="129dp"
            android:layout_marginBottom="6dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="8dp"/>
        <ImageView
            android:id="@+id/rear_radar_left_mid"
            android:layout_width="96dp"
            android:layout_height="141dp"
            android:layout_marginBottom="30dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="92dp"/>
        <ImageView
            android:id="@+id/front_radar_right_mid"
            android:layout_width="79dp"
            android:layout_height="108dp"
            android:layout_marginTop="30dp"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="110dp"/>
        <ImageView
            android:id="@+id/front_radar_right"
            android:layout_width="82dp"
            android:layout_height="83dp"
            android:layout_marginTop="68dp"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="78dp"/>
        <ImageView
            android:id="@+id/right_radar_front"
            android:layout_width="83dp"
            android:layout_height="76dp"
            android:layout_marginTop="108dp"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="58dp"/>
        <ImageView
            android:id="@+id/right_radar_mid_front"
            android:layout_width="68dp"
            android:layout_height="97dp"
            android:layout_marginTop="2dp"
            android:layout_below="@+id/right_radar_front"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="58dp"/>
        <ImageView
            android:id="@+id/right_radar_mid_rear"
            android:layout_width="68dp"
            android:layout_height="97dp"
            android:layout_marginTop="2dp"
            android:layout_below="@+id/right_radar_mid_front"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="58dp"/>
        <ImageView
            android:id="@+id/right_radar_rear"
            android:layout_width="85dp"
            android:layout_height="76dp"
            android:layout_marginTop="2dp"
            android:layout_below="@+id/right_radar_mid_rear"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="58dp"/>
        <ImageView
            android:id="@+id/rear_radar_right"
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:layout_marginBottom="99dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="77dp"/>
        <ImageView
            android:id="@+id/rear_radar_right_auxiliary"
            android:layout_width="101dp"
            android:layout_height="129dp"
            android:layout_marginBottom="5dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="14dp"/>
        <ImageView
            android:id="@+id/rear_radar_right_mid"
            android:layout_width="96dp"
            android:layout_height="141dp"
            android:layout_marginBottom="30dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="92dp"/>
        <ImageView
            android:layout_width="380dp"
            android:layout_height="564dp"
            android:layout_marginTop="26dp"
            android:src="@drawable/__283__radar_bg_1"/>
        <ImageView
            android:id="@+id/imageCar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/__283__dz_radar_car"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/rightLinear"
        android:background="@drawable/dz_radar_r_bg"
        android:layout_width="123dp"
        android:layout_height="600dp"
        android:layout_alignParentRight="true">
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/rightClose"
            android:paddingTop="35dp"
            android:paddingBottom="35dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:dz_no_select_ic="@drawable/dz_radar_r_off_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_off_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_wide"
            android:paddingTop="26dp"
            android:paddingBottom="26dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:dz_no_select_ic="@drawable/dz_radar_r_cara_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_cara_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_standard"
            android:paddingTop="32dp"
            android:paddingBottom="32dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:dz_no_select_ic="@drawable/dz_radar_r_carb_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_carb_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_margin"
            android:paddingTop="25dp"
            android:paddingBottom="25dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:dz_no_select_ic="@drawable/dz_radar_r_carc_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_carc_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_overlook"
            android:paddingTop="18dp"
            android:paddingBottom="18dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            app:dz_no_select_ic="@drawable/dz_radar_r_card_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_card_icon_p"/>
        <View
            android:background="@color/white"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_radar_bg"
            android:paddingTop="22dp"
            android:paddingBottom="22dp"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:dz_no_select_ic="@drawable/dz_radar_r_contrast_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_contrast_icon_p"/>
    </LinearLayout>
    <ImageView
        android:id="@+id/showRightBar"
        android:visibility="gone"
        android:layout_width="102dp"
        android:layout_height="72dp"
        android:layout_marginTop="100dp"
        android:src="@drawable/_283_radar_show"
        android:layout_alignParentEnd="true"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/linearBottomRadar"
        android:background="#cc000000"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_alignParentBottom="true">
        <ImageView
            android:layout_width="46dp"
            android:layout_height="43dp"
            android:src="@drawable/dz_radar_l_icon"
            android:layout_marginStart="80dp"
            android:layout_marginEnd="26dp"/>
        <TextView
            android:textSize="26dp"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="检查车道1"/>
    </LinearLayout>
    <com.hzbhd.canbus.car_cus._283.view.RadarRegulationView
        android:id="@+id/radarRegulationView"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
