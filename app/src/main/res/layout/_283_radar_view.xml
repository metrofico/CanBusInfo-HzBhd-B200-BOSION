<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/relativeLayout"
    android:background="@null"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/showCarRadar"
        android:visibility="gone"
        android:layout_width="@dimen/dp102"
        android:layout_height="@dimen/dp72"
        android:layout_marginTop="@dimen/dp100"
        android:src="@drawable/_283_radar_hide"/>
    <RelativeLayout
        android:id="@+id/LeftRelative"
        android:background="@drawable/dz_radar_r_w_bg"
        android:layout_width="@dimen/dp498"
        android:layout_height="@dimen/dp600">
        <ImageView
            android:id="@+id/leftClose"
            android:visibility="gone"
            android:layout_width="@dimen/dp102"
            android:layout_height="@dimen/dp72"
            android:layout_margin="@dimen/dp10"
            android:src="@drawable/_283_radar_close"
            android:layout_alignParentRight="true"/>
        <ImageView
            android:id="@+id/hideCarRadar"
            android:layout_width="@dimen/dp102"
            android:layout_height="@dimen/dp72"
            android:layout_marginTop="@dimen/dp10"
            android:layout_marginRight="@dimen/dp10"
            android:src="@drawable/_283_radar_show"
            android:layout_below="@+id/leftClose"
            android:layout_alignParentRight="true"/>
        <ImageView
            android:id="@+id/iv_radar_stop"
            android:layout_width="@dimen/dp105"
            android:layout_height="@dimen/dp72"
            android:src="@drawable/_283_radar_2"
            android:layout_above="@+id/iv_radar_voice"
            android:layout_marginEnd="@dimen/dp10"
            android:layout_alignParentEnd="true"/>
        <ImageView
            android:id="@+id/iv_radar_voice"
            android:layout_width="@dimen/dp105"
            android:layout_height="@dimen/dp72"
            android:layout_margin="@dimen/dp10"
            android:src="@drawable/_283_radar_1"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/leftRadarView"
        android:layout_width="@dimen/dp380"
        android:layout_height="@dimen/dp600">
        <ImageView
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/dz_radar_gray_1"
            android:layout_centerInParent="true"
            android:alpha="0.5"/>
        <ImageView
            android:id="@+id/front_radar_left_mid"
            android:layout_width="@dimen/dp79"
            android:layout_height="@dimen/dp108"
            android:layout_marginTop="@dimen/dp30"
            android:layout_marginStart="@dimen/dp110"/>
        <ImageView
            android:id="@+id/front_radar_left"
            android:layout_width="@dimen/dp83"
            android:layout_height="@dimen/dp83"
            android:layout_marginTop="@dimen/dp68"
            android:layout_marginStart="@dimen/dp78"/>
        <ImageView
            android:id="@+id/left_radar_front"
            android:layout_width="@dimen/dp83"
            android:layout_height="@dimen/dp76"
            android:layout_marginTop="@dimen/dp108"
            android:layout_marginStart="@dimen/dp58"/>
        <ImageView
            android:id="@+id/left_radar_mid_front"
            android:layout_width="@dimen/dp69"
            android:layout_height="@dimen/dp97"
            android:layout_marginTop="@dimen/dp2"
            android:layout_below="@+id/left_radar_front"
            android:layout_marginStart="@dimen/dp58"/>
        <ImageView
            android:id="@+id/left_radar_mid_rear"
            android:layout_width="@dimen/dp69"
            android:layout_height="@dimen/dp97"
            android:layout_marginTop="@dimen/dp2"
            android:layout_below="@+id/left_radar_mid_front"
            android:layout_marginStart="@dimen/dp58"/>
        <ImageView
            android:id="@+id/left_radar_rear"
            android:layout_width="@dimen/dp85"
            android:layout_height="@dimen/dp76"
            android:layout_marginTop="@dimen/dp2"
            android:layout_below="@+id/left_radar_mid_rear"
            android:layout_marginStart="@dimen/dp58"/>
        <ImageView
            android:id="@+id/rear_radar_left"
            android:layout_width="@dimen/dp84"
            android:layout_height="@dimen/dp84"
            android:layout_marginBottom="@dimen/dp100"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/dp77"/>
        <ImageView
            android:id="@+id/rear_radar_left_auxiliary"
            android:layout_width="@dimen/dp106"
            android:layout_height="@dimen/dp129"
            android:layout_marginBottom="@dimen/dp6"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/dp8"/>
        <ImageView
            android:id="@+id/rear_radar_left_mid"
            android:layout_width="@dimen/dp96"
            android:layout_height="@dimen/dp141"
            android:layout_marginBottom="@dimen/dp30"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/dp92"/>
        <ImageView
            android:id="@+id/front_radar_right_mid"
            android:layout_width="@dimen/dp79"
            android:layout_height="@dimen/dp108"
            android:layout_marginTop="@dimen/dp30"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="@dimen/dp110"/>
        <ImageView
            android:id="@+id/front_radar_right"
            android:layout_width="@dimen/dp82"
            android:layout_height="@dimen/dp83"
            android:layout_marginTop="@dimen/dp68"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="@dimen/dp78"/>
        <ImageView
            android:id="@+id/right_radar_front"
            android:layout_width="@dimen/dp83"
            android:layout_height="@dimen/dp76"
            android:layout_marginTop="@dimen/dp108"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="@dimen/dp58"/>
        <ImageView
            android:id="@+id/right_radar_mid_front"
            android:layout_width="@dimen/dp68"
            android:layout_height="@dimen/dp97"
            android:layout_marginTop="@dimen/dp2"
            android:layout_below="@+id/right_radar_front"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="@dimen/dp58"/>
        <ImageView
            android:id="@+id/right_radar_mid_rear"
            android:layout_width="@dimen/dp68"
            android:layout_height="@dimen/dp97"
            android:layout_marginTop="@dimen/dp2"
            android:layout_below="@+id/right_radar_mid_front"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="@dimen/dp58"/>
        <ImageView
            android:id="@+id/right_radar_rear"
            android:layout_width="@dimen/dp85"
            android:layout_height="@dimen/dp76"
            android:layout_marginTop="@dimen/dp2"
            android:layout_below="@+id/right_radar_mid_rear"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="@dimen/dp58"/>
        <ImageView
            android:id="@+id/rear_radar_right"
            android:layout_width="@dimen/dp84"
            android:layout_height="@dimen/dp84"
            android:layout_marginBottom="@dimen/dp99"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="@dimen/dp77"/>
        <ImageView
            android:id="@+id/rear_radar_right_auxiliary"
            android:layout_width="@dimen/dp101"
            android:layout_height="@dimen/dp129"
            android:layout_marginBottom="@dimen/dp5"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="@dimen/dp14"/>
        <ImageView
            android:id="@+id/rear_radar_right_mid"
            android:layout_width="@dimen/dp96"
            android:layout_height="@dimen/dp141"
            android:layout_marginBottom="@dimen/dp30"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="@dimen/dp92"/>
        <ImageView
            android:layout_width="@dimen/dp380"
            android:layout_height="@dimen/dp564"
            android:layout_marginTop="@dimen/dp26"
            android:src="@drawable/__283__radar_bg_1"/>
        <ImageView
            android:id="@+id/imageCar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/__283__dz_radar_car"
            android:layout_centerInParent="true"/>
    </RelativeLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/rightLinear"
        android:background="@drawable/dz_radar_r_bg"
        android:layout_width="@dimen/dp123"
        android:layout_height="@dimen/dp600"
        android:layout_alignParentRight="true">
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/rightClose"
            android:paddingTop="@dimen/dp35"
            android:paddingBottom="@dimen/dp35"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="@dimen/dp20"
            android:paddingEnd="@dimen/dp20"
            app:dz_no_select_ic="@drawable/dz_radar_r_off_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_off_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:layout_marginStart="@dimen/dp10"
            android:layout_marginEnd="@dimen/dp10"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_wide"
            android:paddingTop="@dimen/dp26"
            android:paddingBottom="@dimen/dp26"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="@dimen/dp20"
            android:paddingEnd="@dimen/dp20"
            app:dz_no_select_ic="@drawable/dz_radar_r_cara_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_cara_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:layout_marginStart="@dimen/dp10"
            android:layout_marginEnd="@dimen/dp10"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_standard"
            android:paddingTop="@dimen/dp32"
            android:paddingBottom="@dimen/dp32"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="@dimen/dp20"
            android:paddingEnd="@dimen/dp20"
            app:dz_no_select_ic="@drawable/dz_radar_r_carb_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_carb_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:layout_marginStart="@dimen/dp10"
            android:layout_marginEnd="@dimen/dp10"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_margin"
            android:paddingTop="@dimen/dp25"
            android:paddingBottom="@dimen/dp25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="@dimen/dp20"
            android:paddingEnd="@dimen/dp20"
            app:dz_no_select_ic="@drawable/dz_radar_r_carc_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_carc_icon_p"/>
        <View
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:layout_marginStart="@dimen/dp10"
            android:layout_marginEnd="@dimen/dp10"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_overlook"
            android:paddingTop="@dimen/dp18"
            android:paddingBottom="@dimen/dp18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingStart="@dimen/dp20"
            android:paddingEnd="@dimen/dp20"
            app:dz_no_select_ic="@drawable/dz_radar_r_card_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_card_icon_p"/>
        <View
            android:background="@color/white"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp1"
            android:layout_marginStart="@dimen/dp10"
            android:layout_marginEnd="@dimen/dp10"/>
        <com.hzbhd.canbus.car_cus._283.view.BtnView
            android:id="@+id/btn_radar_bg"
            android:paddingTop="@dimen/dp22"
            android:paddingBottom="@dimen/dp22"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:dz_no_select_ic="@drawable/dz_radar_r_contrast_icon_n"
            app:dz_select_ic="@drawable/dz_radar_r_contrast_icon_p"/>
    </LinearLayout>
    <ImageView
        android:id="@+id/showRightBar"
        android:visibility="gone"
        android:layout_width="@dimen/dp102"
        android:layout_height="@dimen/dp72"
        android:layout_marginTop="@dimen/dp100"
        android:src="@drawable/_283_radar_show"
        android:layout_alignParentEnd="true"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:id="@+id/linearBottomRadar"
        android:background="#cc000000"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp59"
        android:layout_alignParentBottom="true">
        <ImageView
            android:layout_width="@dimen/dp46"
            android:layout_height="@dimen/dp43"
            android:src="@drawable/dz_radar_l_icon"
            android:layout_marginStart="@dimen/dp80"
            android:layout_marginEnd="@dimen/dp26"/>
        <TextView
            android:textSize="@dimen/dp26"
            android:textColor="@color/white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="检查车道1"/>
    </LinearLayout>
    <com.hzbhd.canbus.car_cus._283.view.RadarRegulationView
        android:id="@+id/radarRegulationView"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</RelativeLayout>
