<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@drawable/__c6__bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:background="@drawable/_c6_yibiao_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="@dimen/_299_instrument_size"
            android:layout_height="@dimen/_299_instrument_size"
            android:layout_marginTop="@dimen/_299_instrument_margin_top_bottom"
            android:layout_marginStart="@dimen/_299_instrument_margin_left_right">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/_299_seven_color_l_blue_330"/>
            <ImageView
                android:id="@+id/iv_speed_pointer"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/_c6_seven_color_l_pointer_0"/>
            <TextView
                android:textSize="@dimen/dp30"
                android:textColor="@color/white"
                android:id="@+id/tv_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_centerInParent="true"/>
            <TextView
                android:textSize="@dimen/dp12"
                android:textStyle="bold"
                android:textColor="#a9cbe8"
                android:id="@+id/tv_unit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp9"
                android:text="km/h"
                android:layout_below="@+id/tv_speed"
                android:layout_centerHorizontal="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="@dimen/_299_instrument_size"
            android:layout_height="@dimen/_299_instrument_size"
            android:layout_marginTop="@dimen/_299_instrument_margin_top_bottom"
            android:layout_marginEnd="@dimen/_299_instrument_margin_left_right"
            android:layout_alignParentEnd="true">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/_299_seven_color_r_blue"/>
            <ImageView
                android:id="@+id/iv_rotate_speed_pointer"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/_c6_seven_color_r_pointer_0"/>
            <TextView
                android:textSize="@dimen/dp30"
                android:textColor="@color/white"
                android:id="@+id/iv_rotate_speed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <TextView
            android:textSize="@dimen/dp26"
            android:textColor="@color/white"
            android:id="@+id/tv_out_temp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp80"
            android:layout_centerHorizontal="true"/>
        <RelativeLayout
            android:layout_width="@dimen/dp170"
            android:layout_height="@dimen/dp280"
            android:layout_marginBottom="@dimen/_299_instrument_car_bottom"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true">
            <ImageView
                android:layout_width="@dimen/dp90"
                android:layout_height="@dimen/dp197"
                android:src="@drawable/_c6_yibiao_car"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"/>
            <ImageView
                android:id="@+id/iv_bottom_open"
                android:visibility="gone"
                android:layout_width="@dimen/dp58"
                android:layout_height="@dimen/dp22"
                android:src="@drawable/_c6_car_rearbox"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"/>
            <ImageView
                android:id="@+id/iv_top_open"
                android:visibility="gone"
                android:layout_width="@dimen/dp70"
                android:layout_height="@dimen/dp53"
                android:layout_marginBottom="@dimen/dp140"
                android:src="@drawable/_c6_cardoor_qian"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"/>
            <ImageView
                android:id="@+id/iv_top_left_open"
                android:visibility="gone"
                android:layout_width="@dimen/dp45"
                android:layout_height="@dimen/dp50"
                android:layout_marginBottom="@dimen/dp86"
                android:src="@drawable/_c6_cardoor_left1"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/dp16"/>
            <ImageView
                android:id="@+id/iv_bottom_left_open"
                android:visibility="gone"
                android:layout_width="@dimen/dp45"
                android:layout_height="@dimen/dp43"
                android:layout_marginBottom="@dimen/dp36"
                android:src="@drawable/_c6_cardoor_left2"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/dp16"/>
            <ImageView
                android:id="@+id/iv_top_right_open"
                android:visibility="gone"
                android:layout_width="@dimen/dp45"
                android:layout_height="@dimen/dp50"
                android:layout_marginBottom="@dimen/dp86"
                android:src="@drawable/_c6_car_rf_door"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="@dimen/dp16"
                android:layout_alignParentEnd="true"/>
            <ImageView
                android:id="@+id/iv_bottom_right_open"
                android:visibility="gone"
                android:layout_width="@dimen/dp45"
                android:layout_height="@dimen/dp43"
                android:layout_marginBottom="@dimen/dp36"
                android:src="@drawable/_c6_car_rb_door"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="@dimen/dp16"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_299_instrument_tips_bottom"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true">
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/dp170">
                <ImageView
                    android:id="@+id/iv_hand"
                    android:layout_width="@dimen/dp42"
                    android:layout_height="@dimen/dp42"
                    android:src="@drawable/_c6_seven_color_brake_blue"/>
                <TextView
                    android:textSize="@dimen/dp16"
                    android:textColor="@color/white"
                    android:id="@+id/tv_hand"
                    android:layout_width="@dimen/dp66"
                    android:layout_height="wrap_content"
                    android:text="@string/_299_instrument_hand"
                    android:layout_centerHorizontal="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="@dimen/dp16"
                    android:textColor="@color/white"
                    android:id="@+id/tv_safety"
                    android:layout_width="@dimen/dp50"
                    android:layout_height="wrap_content"
                    android:text="@string/_299_instrument_safety"
                    android:layout_marginEnd="@dimen/dp_14"/>
                <ImageView
                    android:id="@+id/iv_safety"
                    android:layout_width="@dimen/dp55"
                    android:layout_height="@dimen/dp42"
                    android:src="@drawable/_c6_seatbelt_blue"/>
            </LinearLayout>
        </LinearLayout>
        <RadioGroup
            android:orientation="horizontal"
            android:id="@+id/radioGroup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp10"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true">
            <RadioButton
                android:enabled="false"
                android:textSize="@dimen/_299_instrument_text_normal"
                android:textColor="@drawable/__299__checked_text_color"
                android:id="@+id/radioButton1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="P"
                android:layout_marginEnd="@dimen/dp16"/>
            <RadioButton
                android:enabled="false"
                android:textSize="@dimen/_299_instrument_text_normal"
                android:textColor="@drawable/__299__checked_text_color"
                android:id="@+id/radioButton2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="N"
                android:layout_marginEnd="@dimen/dp16"/>
            <RadioButton
                android:enabled="false"
                android:textSize="@dimen/_299_instrument_text_normal"
                android:textColor="@drawable/__299__checked_text_color"
                android:id="@+id/radioButton3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="R"
                android:layout_marginEnd="@dimen/dp16"/>
            <RadioButton
                android:enabled="false"
                android:textSize="@dimen/_299_instrument_text_normal"
                android:textColor="@drawable/__299__checked_text_color"
                android:id="@+id/radioButton4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="D"
                android:layout_marginEnd="@dimen/dp16"/>
            <RadioButton
                android:enabled="false"
                android:textSize="@dimen/_299_instrument_text_normal"
                android:textColor="@drawable/__299__checked_text_color"
                android:id="@+id/radioButton5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:button="@null"
                android:text="M"
                android:layout_marginEnd="@dimen/dp16"/>
        </RadioGroup>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
